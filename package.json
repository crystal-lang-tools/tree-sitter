{
	"name": "tree-sitter-crystal",
	"version": "0.1.0",
	"description": "",
	"main": "bindings/node",
	"scripts": {
		"format": "prettier --write ./grammar.js",
		"lint": "eslint grammar.js"
	},
	"keywords": [],
	"author": "Devaune Whittle <https://github.com/devnote-dev>",
	"license": "MIT",
	"dependencies": {
		"nan": "^2.17.0"
	},
	"devDependencies": {
		"eslint": "^8.48.0",
		"prettier": "^2.8.8",
		"tree-sitter-cli": "^0.20.0"
	},
	"tree-sitter": [
		{
			"scope": "source.cr",
			"file-types": [
				"cr"
			],
			"highlights": []
